@model RegistrDisconnection.Models.Dictionaries.Organization

@{
    ViewData["Title"] = "Delete";
}

<h3>Ви хочете видалити</h3>
<div>
    <h4>@Html.DisplayFor(model => model.Name)?</h4>
    <hr />
    <dl class="row">
        <dt class="col-sm-2">
            Код ЦОКу:
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.Code)
        </dd>
        <dt class="col-sm-2">
           Назва ЦОКу:
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.Name)
        </dd>
        <dt class="col-sm-2">
            Начальник:
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.Nach)
        </dd>
        <dt class="col-sm-2">
            Бухгалтер:
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.Buh)
        </dd>
        <dt class="col-sm-2">
            Поштовий індекс:
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.Index)
        </dd>
        <dt class="col-sm-2">
            Повна адреса:
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.Address)
        </dd>
        <dt class="col-sm-2">
            Телефон:
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.Tel)
        </dd>
    </dl>
    <hr />
    <form asp-action="Delete">
        <input type="hidden" asp-for="Id" />
        <input type="submit" value="Видалити" class="btn btn-danger" />
        <a asp-action="Index" class="btn btn-info">Повернутися до списку ЦОКів</a>
    </form>
</div>
